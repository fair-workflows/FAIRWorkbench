@prefix this: <http://www.example.org/workflow1> .
@prefix sub: <http://www.example.org/workflow1#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix pplan: <http://purl.org/net/p-plan#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pwo: <http://purl.org/spar/pwo/> .
@prefix bpmn: <http://dkm.fbk.eu/index.php/BPMN2_Ontology#> .

{
    sub:step1 dul:precedes sub:step2 .
    sub:step2 dul:precedes sub:step3 .
    this: a pplan:Plan ;
       terms:description "This is a test workflow." ;
       rdfs:label "Test workflow" ;
       pwo:hasFirstStep sub:step1 .

    sub:step1 pplan:isStepOfPlan this:;
        a pplan:Step;
        terms:description "Step 1";
        rdfs:label "Step 1" ;
        a bpmn:ManualTask ;
        pplan:hasInputVar sub:input1 ;
        pplan:hasOutputVar sub:output1.

    sub:input1 a pplan:Variable ;
        a "int" .

    sub:output1 a pplan:Variable ;
        a "str" .

    sub:step2 pplan:isStepOfPlan this:;
        a pplan:Step;
        terms:description "Step 2";
        rdfs:label "Step 2" ;
        a bpmn:ManualTask .

    sub:step3 pplan:isStepOfPlan this:;
        a pplan:Step;
        terms:description "Step 3";
        rdfs:label "Step 3" ;
        a bpmn:ScriptTask .
}
